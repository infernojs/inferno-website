{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": [ ">0.4%" ]
        },
        "loose": true
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-inferno",
      {
        "imports": true,
        "defineAllArguments": true
      }
    ],
    "@babel/plugin-transform-async-to-generator"
  ]
}

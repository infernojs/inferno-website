    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "babel-loader": "8.2.2",
    "babel-plugin-inferno": "6.3.0",
    "bootstrap": "5.1.0",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "commonmark": "^0.30.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "debug": "^4.3.2",
    "eslint-plugin-inferno": "7.24.0",
    "fast-async": "^7.0.6",
    "file-loader": "6.2.0",
    "history": "^5.0.1",
    "html-loader": "^2.1.2",
    "http2": "^3.3.7",
    "inferno": "8.0.0-alpha.1",
